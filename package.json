{
  "name": "lubejs",
  "version": "2.0.3",
  "description": "An easy-to-use nodejs oracledb client",
  "main": "dist/index.js",
  "directories": {
    "dist": "dist"
  },
  "bin": {
    "lube": "./dist/bin.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha --fulltrace --full-trace --require ts-node/register --extensions ts,tsx 'test/**/*.{ts,tsx}'",
    "build": "tsc --build src/tsconfig.json && tsc --build drivers/mssql",
    "clean": "pwsh -c 'rm -r -force dist; rm -r -force drivers/mssql/dist'",
    "dep": "npm i oracledb mssql --no-save",
    "lint": "eslint --fix ./src --ext .ts",
    "doc": "typedoc --plugin typedoc-plugin-markdown --out doc src",
    "migrate": "node --require ts-node/register --require tsconfig-paths/register  ./src/bin.ts migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovercao/lubejs.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "oracle",
    "oracledb",
    "mongodb",
    "db",
    "mssql",
    "mysql",
    "database"
  ],
  "author": "jover",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jovercao/lubejs/issues"
  },
  "homepage": "https://github.com/jovercao/lubejs#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^8.2.2",
    "@types/mockjs": "^1.0.3",
    "@types/mssql": "^6.0.8",
    "@types/node": "^14.17.4",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lodash": "^4.17.21",
    "mocha": "^6.2.3",
    "mockjs": "^1.1.0",
    "mssql": "^6.3.2",
    "oracledb": "^5.0.0",
    "power-assert": "^1.6.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.3.1",
    "typeorm": "^0.2.31",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "colors": "^1.4.0",
    "commander": "^7.2.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  }
}
